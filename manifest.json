{
  "name": "Mine Setter",
  "description": "Some rest for your fingers.",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": [
        "https://minesweeper.online/*"
      ],
      "js": [
        "content-script.js"
      ]
    }
  ]
}