{
  "name": "Mine Setter",
  "description": "Some rest for your fingers.",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": [
        "https://minesweeper.online/*"
      ],
      "js": [
        "content-script.js"
      ]
    }
  ],
  "icons": {
    "16": "/images/mine16.png",
    "32": "/images/mine32.png",
    "128": "/images/mine128.png"
  }
}